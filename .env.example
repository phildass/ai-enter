# Razorpay Configuration
# Get your keys from https://dashboard.razorpay.com/app/keys
# Note: RAZORPAY_KEY_SECRET should NEVER be exposed in client-side code
RAZORPAY_KEY_ID=your_razorpay_key_id_here
RAZORPAY_KEY_SECRET=your_razorpay_key_secret_here

# Supabase Configuration
# Get these from your Supabase project settings
# URL format: https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Webhook URLs (for Jai Kisan, Jai Bharat, and IIS Skills backends)
JAI_KISAN_WEBHOOK_URL=https://api.jaikisan.cloud/api/subscription/webhook
JAI_BHARAT_WEBHOOK_URL=https://api.jaibharat.cloud/api/subscription/webhook
IISKILLS_WEBHOOK_URL=https://api.iiskills.cloud/api/subscription/webhook

# Signed handoff token secret (shared with origin sites: jaibharat.cloud, jaikisan.cloud)
# Must be a long random string, kept secret on both the origin backend and ai-enter
HANDOFF_SIGNING_SECRET=your_handoff_signing_secret_here

